{
  "name": "marcojanssen/silex-routing-service-provider",
  "authors": [
    {
      "name": "Marco Janssen",
      "email": "dev@marcojanssen.net"
    },{
      "name": "Samir Rachidi",
      "email": "sr@plusb.de"
    }
  ],
  "keywords": [
    "silex",
    "provider",
    "routing",
    "route"
  ],
