  "require-dev": {
    "phpunit/phpunit" : "4.*",
    "scrutinizer/ocular": "~1.1"
  },
  "autoload": {
    "psr-0": {
      "MJanssen\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "MJanssen\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.4-dev"
    }
  }
